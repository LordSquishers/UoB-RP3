FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

Tinlet          298;

dimensions      [0 0 0 1 0 0 0];

internalField   uniform $Tinlet;

boundaryField
{
    freestream
    {
        type            inletOutlet;
        inletValue      uniform $Tinlet;
        value           $inletValue;
    }

    wall
    {
        type            zeroGradient;
    }

    upperPatch
    {
        type fixedValue;
        value uniform 400;
    }

    lowerPatch
    {
        type fixedValue;
        value uniform 400;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}


